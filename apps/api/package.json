{
  "name": "@wellness/api",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development tsx --tsconfig tsconfig.json src/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "test:rag": "cross-env NODE_ENV=development tsx --tsconfig tsconfig.json src/scripts/testRag.ts",
    "test:vector": "cross-env NODE_ENV=development tsx --tsconfig tsconfig.json src/scripts/testVectorInspection.ts",
    "eval:all": "cross-env NODE_ENV=development node --loader ts-node/esm dist/eval/run-evaluations.js",
    "eval:trajectory": "cross-env NODE_ENV=development node --loader ts-node/esm dist/eval/run-trajectory-eval.js",
    "eval:policy": "cross-env NODE_ENV=development node --loader ts-node/esm dist/eval/run-policy-eval.js",
    "eval:context": "cross-env NODE_ENV=development node --loader ts-node/esm dist/eval/run-context-eval.js",
    "eval:comprehensive": "cross-env NODE_ENV=development node --loader ts-node/esm dist/eval/run-comprehensive-eval.js",
    "eval:validate": "cross-env NODE_ENV=development node dist/eval/validate-dataset.js",
    "eval:test": "cross-env NODE_ENV=development node dist/eval/test-setup.js"
  },
  "dependencies": {
    "@langchain/community": "^0.3.14",
    "@langchain/core": "^0.3.77",
    "@langchain/langgraph": "^0.2.74",
    "@langchain/openai": "^0.3.17",
    "@wellness/dto": "file:../../packages/dto",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "langchain": "^0.3.34",
    "pdf-parse": "^1.1.1",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.11",
    "@types/uuid": "^10.0.0",
    "agentevals": "^0.0.6",
    "cross-env": "^7.0.3",
    "langsmith": "^0.3.69",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.5",
    "typescript": "^5.6.3"
  }
}
